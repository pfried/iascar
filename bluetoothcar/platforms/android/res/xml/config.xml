<?xml version='1.0' encoding='utf-8'?>
<widget id="de.ias.bluetoothcar" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>BluetoothCar</name>
    <description>
        A Cordova Application able to control a IAS Teamwork Car via Bluetooth
    </description>
    <author email="friedrich.maeckle@nantis.de" href="http://nantis.de">
        IAS Universität Stuttgart
        Friedrich Mäckle
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
